# MySQL Docker Configuration Example
# Copy this file to .env and update the values

# MySQL Version
MYSQL_VERSION=8.0

# Container Settings
CONTAINER_NAME=mysql_db
HOSTNAME=mysql-db

# Root Password (Change this!)
ROOT_PASS=your_root_password_here

# Database Settings
DB_NAME=test
DB_USER=test
DB_PASS=your_db_password_here

# Port Mapping
DB_PORT=3306

# Timezone
TZ=Asia/Tokyo
